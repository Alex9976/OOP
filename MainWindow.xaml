<Window x:Class="OOP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOP"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="591,29,0,0" VerticalAlignment="Top" Width="180" Height="40">
            <ComboBoxItem Content="Car"/>
            <ComboBoxItem Content="Bus"/>
            <ComboBoxItem Content="Airplane"/>
            <ComboBoxItem Content="Helicopter"/>
            <ComboBoxItem Content="Boat"/>
        </ComboBox>

    </Grid>
</Window>
